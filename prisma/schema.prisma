generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
  output   = "../prisma-client" 
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Client{
  id        String @id
  name      String
  lastName  String
  phone     String?
  email     String?
  notes     String?
}